<div class="all_users">
  <h1>Users List</h1>

  <table>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th></th>
      <th></th>
    </tr>

    <% @users.each do |user| %>
        <tr>
          <td> <%= image_tag("default_profile.png", :size=>"30x30" ) %> <%= link_to user.username, user %></td>
          <td><%= user.email %></td>
        </tr>
    <% end %>
  </table>

  </br>
  <%= will_paginate @users %>



</div>

